<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;net40;net35</TargetFrameworks>
    <AssemblyName>KaosCollections</AssemblyName>
    <RootNamespace>Kaos.Collections</RootNamespace>
    <Version>0.3.6</Version>
    <Authors>Kasey O.</Authors>
    <Description>Generic collections library for .NET containing RankedDictionary and RankedSet that are replacements for SortedDictionary and SortedSet with improved performance and more capability.</Description>
    <AssemblyVersion>0.3.6.0</AssemblyVersion>
    <FileVersion>0.3.6.0</FileVersion>
    <Company>github.com/kaosborn</Company>
    <PackageId>Kaos.Collections</PackageId>
    <Copyright>Copyright © 2009-2017 github.com/kaosborn</Copyright>
    <PackageProjectUrl>https://github.com/kaosborn/KaosCollections/wiki</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kaosborn/KaosCollections.git</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes>See GitHub page for full documentation (.CHM file) and issue tracking.</PackageReleaseNotes>
    <PackageTags>Generics Collections SortedDictionary SortedSet List BTree Order Statistic</PackageTags>
    <PackageLicenseUrl>https://github.com/kaosborn/KaosCollections/blob/master/LICENSE.txt</PackageLicenseUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard1.0|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.0\KaosCollections.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>
  </PropertyGroup>

  <Import Project="..\Source\RankedSet\RankedSet.projitems" Label="Shared" />

  <Import Project="..\Source\RankedDictionary\RankedDictionary.projitems" Label="Shared" />

  <Import Project="..\Source\Btree\Btree.projitems" Label="Shared" />

</Project>